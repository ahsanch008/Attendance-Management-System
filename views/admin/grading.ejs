<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User Grading</title>
    <link rel="stylesheet" href="/css/adminAction.css">
</head>
<body>
    <h1>User Grading</h1>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Grade</th>
            </tr>
        </thead>
        <tbody>
            <% gradedUsers.forEach(user => { %>
                <tr>
                    <td><%= user.fullName %></td>
                    <td><%= user.email %></td>
                    <td><%= user.grade %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
